{
  "name": "ng2-boaster",
  "version": "0.1.0",
  "description": "An Angular.io Notification Module (with Bootstrap 4)",
  "main": "index.html",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "gulp": "gulp sass",
    "minify": "uglifyjs dist/bundles/boaster.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/boaster.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skyl0/ng2-boaster.git"
  },
  "keywords": [
    "angular",
    "notifications",
    "bootstrap4"
  ],
  "author": "Marc Ernst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Skyl0/ng2-boaster/issues"
  },
  "homepage": "https://github.com/Skyl0/ng2-boaster#readme",
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/common": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.4.6",
    "gulp": "^3.9.1",
    "jquery": "^3.0.0",
    "popper.js": "^1.11.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.8",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "bootstrap": "^4.0.0-beta",
    "gulp-sass": "^3.1.0",
    "rollup": "^0.50.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.1.4"
  }
}
